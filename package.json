{
  "name": "react-template",
  "packageManager": "yarn@3.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env SERVE=true webpack serve --config ./configs/webpack.config.dev.js",
    "start:mock": "cross-env SERVE=true MOCK_API=true webpack serve --config ./configs/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.prod.js",
    "build:analyze": "cross-env ANALYZE=true yarn build",
    "build:dev": "webpack --config ./configs/webpack.config.dev.js",
    "lint": "eslint -c ./configs/.eslintrc.js --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint-staged": "lint-staged -c ./configs/lint-staged.config.js",
    "postinstall": "husky install",
    "sdk": "yarn dlx @yarnpkg/sdks vscode"
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^3.1.1-rc.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/babel__core": "^7",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/mini-css-extract-plugin": "^2",
    "@types/prettier": "^2",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-redux": "^7",
    "@types/sass": "^1",
    "@types/webpack-bundle-analyzer": "^4",
    "@types/webpack-dev-server": "^4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-jsx-conditionals": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "mini-css-extract-plugin": "^2.4.4",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.0.0",
    "prettier": "2.4.1",
    "react-refresh": "^0.11.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0",
    "~": "link:./src"
  },
  "prettier": "./configs/prettier.config.js",
  "browserslist": [
    "last 2 versions",
    "> 0.5%",
    "IE 10"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint -c ./configs/.eslintrc.js --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
